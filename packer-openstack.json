{
  "variables": {
    "os_auth_url": "",
    "os_image_name": "packer-cloudbreak-centos-{{ isotime \"2006-01-02\" }}",
    "os_username": "",
    "os_password": "",
    "os_tenant_name": ""
  },
  "builders": [
    {
      "type": "openstack",
      "username": "{{user `os_username`}}",
      "password": "{{user `os_password`}}",
      "project": "{{user `os_tenant_name`}}",
      "ssh_username": "centos",
      "provider": "{{user `os_auth_url`}}",
      "image_name": "{{user `os_image_name`}}",
      "source_image": "221849cd-cd1a-4c57-ae0e-5fa993642e88",
      "flavor": "2",
      "use_floating_ip": true,
      "networks": [
        "14be4c61-286e-4909-b625-e2eebc72a6d3"
      ]
    }
  ],
  "provisioners": [{
    "type": "file",
    "source": "scripts/",
    "destination": "/tmp"
  },
  {

    "type": "shell",
    "script": "./user-data-script.sh",
    "execute_command": "chmod +x {{ .Path }}; {{ .Vars }} sudo -E bash '{{ .Path }}'"
  }]
}
